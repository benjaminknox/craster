doctype html
html
  head
    title Viewer
    style.
      html, body { margin: 0; padding: 0; font: 11px sans-serif; background: white }
  body
    canvas(
      width= width
      height= height
      data-3dviewer-url= url
      data-3dviewer-x= x
      data-3dviewer-y= y
      data-3dviewer-z= z
    )

    != js("jsc3d")
    != js("viewer")
